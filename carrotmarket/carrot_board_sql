create sequence carrot_seq_board;

create table carrot_board (
  bno number(10,0),
  title varchar2(200) not null,
  content varchar2(3000) not null,
  writer varchar2(50) not null,
  regdate date default sysdate, 
  updatedate date default sysdate
);


alter table carrot_board add constraint carrot_pk_board primary key (bno);

insert into carrot_board (bno, title, content, writer) 
values (carrot_seq_board.nextval, '테스트 제목','테스트 내용','당근유저');

commit;

